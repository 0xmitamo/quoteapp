 <div id="search-square">
    <% @rhetorics.each do |rhetoric| %>
      <% rhetoric.tag_list.each do |tag| %>
        <%= link_to rhetorics_path(tag_name: tag) do %>
          <div class="tag-body">
            <%= "#{tag}" %>
          </div>
        <% end %>
      <% end %>
    <% end %>
  </div>
